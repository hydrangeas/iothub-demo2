{
  "CollectorConfig": {
    "MonitoringPaths": [],
    "FileFilter": "*.jsonl",
    "StabilizationPeriodSeconds": 5,
    "MaxConcurrency": 4,
    "RetentionPolicy": {
      "RetentionDays": 7,
      "LargeFileRetentionDays": 30,
      "LargeFileSizeThreshold": 52428800,
      "ArchiveDirectoryPath": "archive",
      "CompressProcessedFiles": true
    }
  },
  "BatchConfig": {
    "MaxBatchSizeBytes": 1048576,
    "MaxBatchItems": 10000,
    "ProcessingIntervalSeconds": 30,
    "RetryPolicy": {
      "MaxRetries": 5,
      "InitialRetryIntervalSeconds": 1,
      "MaxRetryIntervalSeconds": 30,
      "RetryBackoffMultiplier": 2.0
    }
  },
  "IoTHubConfig": {
    "ConnectionString": "",
    "DeviceId": "",
    "SasToken": null,
    "UploadFolderPath": "logs",
    "FileUpload": {
      "SasTokenTimeToLiveMinutes": 60,
      "EnableNotification": true,
      "LockDurationMinutes": 1,
      "DefaultTimeToLiveDays": 1,
      "MaxDeliveryCount": 10
    }
  },
  "KeyVaultUri": "",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  }
}
